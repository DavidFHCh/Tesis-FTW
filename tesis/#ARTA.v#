Require Import List.
Require Import Utf8.
Require Import Nat.

Variable A : Type.
 
(*Empezamos con la implementaciÃ³n de arboles rojinegros.*)
(*What is the diff between data and type in haskell, me noob.*)


Inductive EmptyT (A:Type): Set :=
| E : EmptyT A.

Inductive tree (A: Type) : Type :=
  | Leaf : A -> tree A
  | Node : tree A -> tree A -> tree A.

Check tree.

Definition TreeType := Type -> Type.
Check TreeType.

Definition Tr (T: TreeType) (A : Type) := 
prod (prod (T A) A) (T A).

Check Tr.



